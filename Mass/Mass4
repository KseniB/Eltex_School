#include <stdio.h>

int main()
{
    const int N = 5;
    int A[N][N];
    int i = 0;
    int j = 0;
    int k = 1;
    while (k <= N*N){
        A[i][j] = k;
        if (i <= j + 1 && i + j < N - 1){
            j++;
        }
        else if (i < j && i + j >= N - 1){
            i++;
        }
        else if (i >= j && i + j > N - 1){
            j--;
        }    
        else{ 
            i--;
        }
        k++;
    }
    for (int i = 0; i < N; i++){
        for (int j = 0; j < N; j++){
            printf("%3d", A[i][j]);
        }
        printf("\n");
    }
    return 0;
}
