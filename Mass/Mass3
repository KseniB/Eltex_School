#include <stdio.h>

int main()
{
    const int N = 5;
    int A[N][N];
    for (int i = 0; i < N; i++){
        for (int j = 0; j < N; j++){
            if ((i + j) >= N){
                A[i][j] = 1;   
            } else {
                A[i][j] = 0;
            }
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }
    return 0;
}
